<div class="container-fluid">
  <div class="row">
    <div class="col-md-6">
        <h3>GET resources</h3>
        <ol>
            <li> <a href="http://localhost:3004/sec/findall">http://localhost:3004/sec/findall</a></li>
            <li> <a href="http://localhost:3004/sec/findone/1">http://localhost:3004/sec/findone/1</a></li>
            <li> <a href="http://localhost:3004/sec/">http://localhost:3004/sec/</a></li>
            <li> <a href="http://localhost:3004/sec/create">http://localhost:3004/sec/create</a></li>
            <li> <a href="http://localhost:3004/sec/details">http://localhost:3004/sec/details</a></li>
            <li> <a href="http://localhost:3004/sec/edit">http://localhost:3004/sec/edit</a></li>
            <li> <a href="http://localhost:3004/sec/delete">http://localhost:3004/sec/delete</a></li>
        </ol>
    </div>
    <div class="col-md-6">
            <h3>POST resources</h3>
            <ol>
                <li> <a href="http://localhost:3004/sec/">http://localhost:3004/sec/</a></li>
                <li> <a href="http://localhost:3004/sec/create">http://localhost:3004/sec/create</a></li>
                <li> <a href="http://localhost:3004/sec/details">http://localhost:3004/sec/details</a></li>
                <li> <a href="http://localhost:3004/sec/edit">http://localhost:3004/sec/edit</a></li>
                <li> <a href="http://localhost:3004/sec/delete">http://localhost:3004/sec/delete</a></li>
            </ol>
        </div>
    <div class="col-md-3">
      <a href="sec/create" class="btn btn-primary new">
        add a new section
      </a>
      
      <h2>Recent entries</h2>
      <p>There are
        <%= sections.length %> sections.
        <br>
      </p>
      <% for (var i = sections.length-1; i >= 0; i--) {   
        section = sections[i];   %>
      <div class="panel panel-default">
        <div class="panel-heading"></div>
        <div class="text-muted pull-right"></div>
        <div class="panel-body">

          <br> ID: <%=  section._id %>
          <br> Section Number: <%=section.SectionNumber%>
          <br> Days: <%=section.Days%>
          <br> Start Time: <%=section.StartTime%>
          <br> Room Number: <%=section.RoomNumber%>
          <br> Instructor ID: <%=section.InstructorID%>
          <br> Course ID: <%=section.CourseID%>
          <hr>
          <a href="/sec/delete/<%= section._id  %>" class="btn btn-danger"> Delete </a>
          <a href="/sec/details/<%= section._id %>" class="btn btn-info"> Details </a>
          <a href="/sec/edit/<%= section._id %>" class="btn btn-primary"> Edit </a>
          <hr>
          <hr>
        </div>
      </div>
      <% } %>

    </div>
  </div>
</div>