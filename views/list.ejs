<select>
    <% for (var i = courses.length-1; i >= 0; i--) {   
      course = courses[i];   %>
      <option value=<%=String(course.SchoolNumber) + String(course.CourseNumber) %>>
        <%= course.Name + " - " + String(course.SchoolNumber) + String(course.CourseNumber)%>
      </option>
    <% } %>
</select>